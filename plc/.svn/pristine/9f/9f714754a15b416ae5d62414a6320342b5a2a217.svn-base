    private function ViewUPB ($id=0){
        $upb = $this->db->get_where('plc2.'.$this->main_table, array($this->main_table_pk=>$id, 'ldeleted'=>0))->result_array();
        $arrUPB = array();
        foreach ($upb as $u) {
            if (isset($u['iupb_id'])){
                array_push($arrUPB, $u['iupb_id']);
            }
        }
        return $arrUPB;
    }

