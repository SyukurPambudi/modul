<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rpt_history_upb" language="groovy" pageWidth="6500" pageHeight="2380" orientation="Landscape" whenNoDataType="AllSectionsNoDetail" columnWidth="6500" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="5506"/>
	<property name="ireport.y" value="0"/>
	<parameter name="iupb_id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[0]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="iteambusdev" class="java.lang.String">
		<defaultValueExpression><![CDATA[4]]></defaultValueExpression>
	</parameter>
	<parameter name="iteampd" class="java.lang.String">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	#No UPB
	p.iupb_id as ID,
	p.iupb_id as iupb_id,
	p.vupb_nomor  as vupb_nomor,
	p.vupb_nama as vupb_nama,

	#Approval Busdev
	( SELECT(IF (p.iappbusdev = 2, concat (
		'Approve',' Oleh: ',(select cnip from plc2.plc2_upb_approve where vmodule in ('AppUPB-BD','2471') and vtipe='BD' and iupb_id=ID and ldeleted=0 limit 1),
		  ' Pada: ',(select tupdate from plc2.plc2_upb_approve where vmodule in ('AppUPB-BD','2471') and vtipe='BD' and iupb_id=ID and ldeleted=0 limit 1)
		),
				IF(p.iappbusdev = 1, concat (
		'Reject',' Oleh: ',(select cnip from plc2.plc2_upb_approve where vmodule in ('AppUPB-BD','2471') and vtipe='BD' and iupb_id=ID and ldeleted=0 limit 1),
		  ' Pada: ',(select tupdate from plc2.plc2_upb_approve where vmodule in ('AppUPB-BD','2471') and vtipe='BD' and iupb_id=ID and ldeleted=0 limit 1)
		), '  ')
				)
			)
	) as 'appbus',

	#Approval Direksi
	( SELECT(IF (p.iappdireksi = 2, concat (
		'Approve',' Oleh: ',(select cnip from plc2.plc2_upb_approve where vmodule in ('AppUPB-DR','2471') and vtipe='DR' and iupb_id=ID and ldeleted=0 limit 1),
		  ' Pada: ',(select tupdate from plc2.plc2_upb_approve where vmodule in ('AppUPB-DR','2471') and vtipe='DR' and iupb_id=ID and ldeleted=0 limit 1)
		),
				IF(p.iappdireksi = 1, concat (
		'Reject',' Oleh: ',(select cnip from plc2.plc2_upb_approve where vmodule in ('AppUPB-DR','2471') and vtipe='DR' and iupb_id=ID and ldeleted=0 limit 1),
		  ' Pada: ',(select tupdate from plc2.plc2_upb_approve where vmodule in ('AppUPB-DR','2471') and vtipe='DR' and iupb_id=ID and ldeleted=0 limit 1),
		  ' Alasan: ',(select treason from plc2.plc2_upb_approve where vmodule in ('AppUPB-DR','2471') and vtipe='DR' and iupb_id=ID and ldeleted=0 limit 1)
		), '  ')
				)
			)
	) as 'appdir',

	#Prioritas PraReg
 	(select
		concat (
		(case
		  when pr.iappbusdev=1 then 'Reject'
		  when pr.iappbusdev=2 then'Approve'
		  else 'Waiting For Approval'
		  end),' Oleh: ',pr.cnip,
		  ' Pada: ', pr.tappbusdev
	)
	from  plc2.plc2_upb_prioritas_detail prd
	inner join plc2.plc2_upb_prioritas pr on pr.iprioritas_id=prd.iprioritas_id and pr.ldeleted=0
	where prd.iupb_id=ID and prd.ldeleted=0
	order by pr.iyear desc limit 1)
	as 'setpraapp',

		#Input Sample Originator
	ifnull((select sor.dTanggalKirimBD 
	from plc2.plc2_upb_date_sample sor where sor.iupb_id=ID
	order by sor.iCounter desc limit 1),'')as tglOriBD,

	ifnull((select cPengirimBD 
	from plc2.plc2_upb_date_sample sor where sor.iupb_id=ID
	order by sor.iCounter desc limit 1),'')as pengirimOriBD,

	ifnull((select sor.dTanggalTerimaPD
	from plc2.plc2_upb_date_sample sor where sor.iupb_id=ID
	order by sor.iCounter desc limit 1),'') as tglOriPD,
	ifnull((select sor.cPenerimaPD
	from plc2.plc2_upb_date_sample sor where sor.iupb_id=ID
	order by sor.iCounter desc limit 1),'') as penerimaOriPD,

	ifnull((select sor.dTanggalTerimaAD
	from plc2.plc2_upb_date_sample sor where sor.iupb_id=ID
	order by sor.iCounter desc limit 1),'') as tglOriAD,
	ifnull((select sor.cPenerimaAD
	from plc2.plc2_upb_date_sample sor where sor.iupb_id=ID
	order by sor.iCounter desc limit 1),'') as penerimaOriAD,


	ifnull((select sor.dTanggalTerimaQA
	from plc2.plc2_upb_date_sample sor where sor.iupb_id=ID
	order by sor.iCounter desc limit 1),'') as tglOriQA,
	ifnull((select sor.cPenerimaQA
	from plc2.plc2_upb_date_sample sor where sor.iupb_id=ID
	order by sor.iCounter desc limit 1),'') as penerimaOriQA,


	#ambil id po dan id request sample
	( select sm3.ireq_id from  plc2.plc2_upb_request_sample sm3 where
		sm3.iupb_id =ID and ldeleted = 0   order by sm3.ireq_id desc limit 1
	) as iReqid,
	( select po1.ipo_id from plc2.plc2_upb_po_detail po1 where po1.ireq_id =
		iReqid and ldeleted = 0  order by po1.ipo_id desc limit 1) as ID_PO,

	#Permintaan Sample
	(select
		concat (
				(case
				  when rs.iapppd=1 then 'Reject'
				  when rs.iapppd=2 then 'Approve'
				  end),' Oleh: ',rs.vnip_apppd,
				  ' Pada: ',rs.tapppd
			)
		 from plc2.plc2_upb_request_sample rs
			where rs.ireq_id=iReqid) as appreqsample,

		#PO Sample
		(select
					concat (
								(case
								  when po.iapppr=1 then 'Reject'
								  when po.iapppr=2 then 'Approve'
								  end),' Oleh: ',po.vnip_pur,
								  ' Pada: ',po.tapp_pur
							)
			 from plc2.plc2_upb_po po
				where po.ipo_id in 
				(select po1.ipo_id from plc2.plc2_upb_po_detail po1 
					inner join plc2.plc2_upb_po po2 on po1.ipo_id=po2.ipo_id
					where po1.ireq_id =iReqid and po1.ldeleted = 0 and po2.iapppr in 
					(if((select count(po1.ipo_id) from plc2.plc2_upb_po_detail po1 where po1.ireq_id =iReqid 
					and po1.ldeleted = 0 order by po1.ipo_id desc)>=2,2,1),
					if((select count(po1.ipo_id) from plc2.plc2_upb_po_detail po1 where po1.ireq_id =iReqid 
					and po1.ldeleted = 0 order by po1.ipo_id desc)>=2,0,2)) order by po1.ipo_id desc)	
				and po.iapppr in 
				(if((select count(po1.ipo_id) from plc2.plc2_upb_po_detail po1 where po1.ireq_id =iReqid 
					and ldeleted = 0 order by po1.ipo_id desc)>=2,2,1),
					if((select count(po1.ipo_id) from plc2.plc2_upb_po_detail po1 where po1.ireq_id =iReqid 
					and po1.ldeleted = 0 order by po1.ipo_id desc)>=2,0,2)) order by po.ipo_id desc limit 1) as appro_pr,

		#Penerimaan Sample
		(select
			concat (
				(case
				  when ro.iapppr=1 then 'Reject'
				  when ro.iapppr=2 then 'Approve'
			   end),' Oleh: ',ro.vnip_pur,
				' Pada: ',ro.tapp_pur
				  )
		   from plc2.plc2_upb_ro ro
		  where
		 	 ro.iro_id=(select rod.iro_id from plc2.plc2_upb_ro_detail rod
				where rod.ipo_id=ID_PO order by rod.ipo_id desc limit 1
										)
								) as apprecv_pr,

		# PD Terima sample bahan baku
		(select
	concat (
		' Oleh: ',rod.vrec_nip_pd,
		' Pada: ',rod.trec_date_pd
		  )
		from plc2.plc2_upb_ro_detail rod
		where rod.ipo_id=ID_PO
		and rod.ldeleted=0
		order by rod.irodet_id desc limit 1
		) as rec_pd,

		# QC Terima sample bahan baku
	(select
	concat (
		' Oleh: ',rod.vrec_nip_qc,
		' Pada: ',rod.trec_date_qc
		  )
		from plc2.plc2_upb_ro_detail rod
		where rod.ipo_id=ID_PO
		and rod.ldeleted=0
		order by rod.irodet_id desc limit 1
		) as rec_qc,

		# QA Terima sample bahan baku
	(select
	concat (
		' Oleh: ',rod.vrec_nip_qa,
		' Pada: ',rod.trec_date_qa
		  )
		from plc2.plc2_upb_ro_detail rod
		where rod.ipo_id=ID_PO
		and rod.ldeleted=0
		order by rod.irodet_id desc limit 1
		) as rec_qa,

		#PD App Analisa Sample Bahan Baku
		(select
		concat (
			(case
			  when rod.iapppd_analisa=1 then 'Reject'
			  when rod.iapppd_analisa=2 then 'Approve'
		   end),' Oleh: ',rod.vnip_apppd_analisa,
			' Pada: ',rod.tapppd_analisa
			  )
			from plc2.plc2_upb_ro_detail rod
			where rod.ipo_id=ID_PO and rod.ldeleted=0	order by rod.irodet_id desc limit 1
		  ) as apppd_analisa,

  		#PD App Release Bahan Baku
		 (select
			concat (
				(case
				  when rod.iapppd_rls=1 then 'Reject'
				  when rod.iapppd_rls=2 then 'Approve'
			   end),' Oleh: ',rod.vnip_apppd_rls,
				' Pada: ',rod.tapppd_rls
				  )
				from plc2.plc2_upb_ro_detail rod
				where rod.ipo_id=ID_PO and rod.ldeleted=0
				order by rod.irodet_id desc limit 1
			) as apppd_rls,

		#SOI Bahan Baku QA
		(select
			concat (
			(case
			  when f4.iappqa=1 then 'Reject'
			  when f4.iappqa=2 then'Approve'
			  else 'Waiting For Approval'
			  end),' Oleh: ',f4.vnip_qa,
			  ' Pada: ',f4.tapp_qa
			)
	 	from plc2.plc2_upb_soi_bahanbaku f4
		where f4.iupb_id=$P{iupb_id} order by f4.isoibb_id desc limit 1) as appsoibbqa,

		#SOI Bahan Baku QC
		(select
			concat (
			(case
			  when f4.iappqc=1 then 'Reject'
			  when f4.iappqc=2 then'Approve'
			  else 'Waiting For Approval'
			  end),' Oleh: ',f4.vnip_qc,
			  ' Pada: ',f4.tapp_qc
			)
	 	from plc2.plc2_upb_soi_bahanbaku f4
		where f4.iupb_id=$P{iupb_id} order by f4.isoibb_id desc limit 1) as appsoibbqc,

	# simpan id formula sementara
	(select fo.ifor_id from plc2.plc2_upb_formula fo
	where fo.iupb_id=ID
	order by fo.ifor_id desc limit 1 ) as FOR_ID,

	#App PD Formula Skala Trial
	(select
		concat (
		ifnull((case
		  when fo.iformula_apppd=1 then 'Reject'
		  when fo.iformula_apppd=2 then'Approve'
		  else 'Waiting for Approval'
		  end),'-'),' Oleh: ',ifnull(fo.vformula_nip_apppd,'-'),
		  ' Pada: ',ifnull(fo.tformula_apppd,'-')
	)
 	from plc2.plc2_upb_formula fo
	where fo.ifor_id=FOR_ID) as appforpd,

	#App PD Stress Test
	(select
		concat (
		ifnull((case
		  when fo.istress_apppd=1 then 'Reject'
		  when fo.istress_apppd=2 then'Approve'
		  else 'Waiting for Approval'
		  end),'-'),' Oleh: ',ifnull(fo.vstress_nip_apppd,'-'),
		  ' Pada: ',ifnull(fo.tstress_apppd,'-')
	)
 	from plc2.plc2_upb_formula fo
	where fo.ifor_id=FOR_ID) as appstpd,

	#App PD Formula Skala Lab
	(select
		concat (
		ifnull((case
		  when fo.ilab_apppd=1 then 'Reject'
		  when fo.ilab_apppd=2 then'Approve'
		  else 'Waiting for Approval'
		  end),'-'),' Oleh: ',ifnull(fo.vlab_nip_apppd,'-'),
		  ' Pada: ',ifnull(fo.tlab_apppd,'-')
		)
	from plc2.plc2_upb_formula fo
	where fo.ifor_id=FOR_ID) as applabpd,

	#QA App KSK
		 (select
			concat (
				(case
				  when rod.iappqa_ksk=1 then 'Reject'
				  when rod.iappqa_ksk=2 then 'Approve'
			   end),' Oleh: ',rod.vnip_appqa_ksk,
				' Pada: ',rod.tappqa_ksk
				  )
				from plc2.plc2_upb_ro_detail rod
				where rod.ipo_id=ID_PO
				order by rod.irodet_id desc limit 1
			) as apppd_ksk,



	#stabilita Lab 0
	(select
		concat(
		ifnull((case
			  when ts.iapppd=1 then 'Reject'
			  when ts.iapppd=2 then'Approve'
			  else 'Waiting for Approval'
			  end),'-'),' Oleh: ',ifnull(ts.vnip_apppd,'-'),
			  ' Pada: ',ifnull(ts.tapppd,'-')
		)
	from plc2.plc2_upb_stabilita ts
	where
 	ts.ifor_id=FOR_ID and ts.inumber=0 order by ts.ista_id desc limit 1) as stab0,

	 #stabilita Lab 1
	(select
		concat(
		ifnull((case
			  when ts.iapppd=1 then 'Reject'
			  when ts.iapppd=2 then'Approve'
			  else 'Waiting for Approval'
			  end),'-'),' Oleh: ',ifnull(ts.vnip_apppd,'-'),
			  ' Pada: ',ifnull(ts.tapppd,'-')
		)
	from plc2.plc2_upb_stabilita ts
	where
 	ts.ifor_id=FOR_ID and ts.inumber=1 order by ts.ista_id desc limit 1) as stab1,

	 #stabilita Lab 3
	(select
		concat(
		ifnull((case
			  when ts.iapppd=1 then 'Reject'
			  when ts.iapppd=2 then'Approve'
			  else 'Waiting for Approval'
			  end),'-'),' Oleh: ',ifnull(ts.vnip_apppd,'-'),
			  ' Pada: ',ifnull(ts.tapppd,'-')
		)
	from plc2.plc2_upb_stabilita ts
	where
 	ts.ifor_id=FOR_ID and ts.inumber=3 order by ts.ista_id desc limit 1) as stab3,

 	#stabilita Lab 6
	(select
		concat(
		ifnull((case
			  when ts.iapppd=1 then 'Reject'
			  when ts.iapppd=2 then'Approve'
			  else 'Waiting for Approval'
			  end),'-'),' Oleh: ',ifnull(ts.vnip_apppd,'-'),
			  ' Pada: ',ifnull(ts.tapppd,'-')
		)
	from plc2.plc2_upb_stabilita ts
	where
 	ts.ifor_id=FOR_ID	and ts.inumber=6 order by ts.ista_id desc limit 1) as stab6,

 	#Best Formula Approval
 	(select
		(case
		  when fo.ibest=1 then ' '
		  when fo.ibest=2 then (concat (' Oleh: ',ifnull(fo.vbest_nip_apppd,'-'),
		  ' Pada: ',ifnull(fo.tbest_apppd,'-')))
		  else ''
		  end)
 	from plc2.plc2_upb_formula fo
	where fo.ifor_id=FOR_ID) as appbestpd,

	#spek FG PD
		(select
			concat (
			(case
			  when f1.iapppd=1 then 'Reject'
			  when f1.iapppd=2 then'Approve'
			  else 'Waiting For Approval'
			  end),' Oleh: ',f1.vnip_pd,
			  ' Pada: ',f1.tapp_pd
			)
	 	from plc2.plc2_upb_spesifikasi_fg f1
		where f1.iupb_id=ID order by f1.ispekfg_id desc limit 1) as appsfgpd,

		#spek FG QA
		(select
			concat (
			(case
			  when f2.iappqa=1 then 'Reject'
			  when f2.iappqa=2 then'Approve'
			  else 'Waiting For Approval'
			  end),' Oleh: ',f2.vnip_qa,
			  ' Pada: ',f2.tapp_qa
			)
	 	from plc2.plc2_upb_spesifikasi_fg f2
		where f2.iupb_id=$P{iupb_id} order by f2.ispekfg_id desc limit 1) as appsfgqa,

		#SOI FG QA
		(select
			concat (
			(case
			  when f3.iappqa=1 then 'Reject'
			  when f3.iappqa=2 then'Approve'
			  else 'Waiting For Approval'
			  end),' Oleh: ',f3.vnip_qa,
			  ' Pada: ',f3.tapp_qa
			)
	 	from plc2.plc2_upb_soi_fg f3
		where f3.iupb_id=ID order by f3.isoi_id desc limit 1) as appsofgqa,

		#SOI FG Tentative QC
		(select
			concat (
			(case
			  when f4.iappqc=1 then 'Reject'
			  when f4.iappqc=2 then'Approve'
			  else 'Waiting For Approval'
			  end),' Oleh: ',f4.vnip_qc,
			  ' Pada: ',f4.tapp_qc
			)
	 	from plc2.plc2_upb_soi_fg f4
		where f4.iupb_id=$P{iupb_id} order by f4.isoi_id desc limit 1) as appsofgqc,

		#SOI Mikro Tentative QA
		(select
			concat (
			(case
			  when f5.iappqa=1 then 'Reject'
			  when f5.iappqa=2 then'Approve'
			  else 'Waiting For Approval'
			  end),' Oleh: ',f5.vnip_qa,
			  ' Pada: ',f5.tapp_qa
			)
	 	from plc2.plc2_upb_mikro_fg f5
		where f5.iupb_id=$P{iupb_id} order by f5.imikro_id desc limit 1) as appmikroqa,

	#App HPP PD
	(select
		(concat (' Oleh: ',ifnull(hp.vnip_apppd,'-'),
			  ' Pada: ',ifnull(hp.tapppd,'-')))
		 from plc2.plc2_hpp hp where hp.ifor_id=FOR_ID
	 ) as apphpp_pd,

	 #App HPP FA
	 	(select
		(concat (' Oleh: ',ifnull(hp.vnip_appfa,'-'),
			  ' Pada: ',ifnull(hp.tappfa,'-')))
	 from plc2.plc2_hpp hp where hp.ifor_id=FOR_ID
	 ) as apphpp_fa,

 		#App HPP BD
	 	(select
		(concat (' Oleh: ',(case
			  when hp.vnip_appbd='-' then 'Tidak Memerlukan'
			  when hp.vnip_appbd='null' then '-'
		   end),
			  ' Pada: ',ifnull(hp.tappbd,'-')))
		 from plc2.plc2_hpp hp where hp.ifor_id=FOR_ID
		 ) as apphpp_bd,

  		#App HPP Dir
	  	(select
			(concat (' Oleh: ',ifnull(hp.vnip_appdir,'-'),
				  ' Pada: ',ifnull(hp.tappdir,'-')))
		 from plc2.plc2_hpp hp where hp.ifor_id=FOR_ID
		 ) as apphpp_dir,

		 #App Basic Formula PD
		(select
			(case
			  when fo.iapp_basic=1 then ' '
			  when fo.iapp_basic=2 then (concat (' Oleh: ',ifnull(fo.vbasic_nip_apppd,'-'),
			  ' Pada: ',ifnull(fo.tbasic_apppd,'-')))
			  else ''
			  end)
	 		from plc2.plc2_upb_formula fo
		where fo.ifor_id=FOR_ID) as appbasicpd,

			#Bahan Kemas Appp PD
		(select
			concat (
					(case
					  when bk.iapppd=1 then 'Reject'
					  when bk.iapppd=2 then 'Approve'
					  end),' Oleh: ',bk.vnip_apppd,
					  ' Pada: ',bk.tapppd
				)
		 from plc2.plc2_upb_bahan_kemas bk
			where bk.iupb_id=ID
			order by bk.ibk_id desc limit 1) as bkapp_pd,

		#Bahan Kemas Appp QA
		(select
				concat (
						(case
						  when bk.iappqa=1 then 'Reject'
						  when bk.iappqa=2 then 'Approve'
						  end),' Oleh: ',bk.vnip_appqa,
						  ' Pada: ',bk.tappqa
					)
		 from plc2.plc2_upb_bahan_kemas bk
			where bk.iupb_id=ID
			order by bk.ibk_id desc limit 1) as bkapp_qa,

		#Bahan Kemas Appp BD
		(select
			concat (
					(case
					  when bk.iappbd=1 then 'Reject'
					  when bk.iappbd=2 then 'Approve'
					  end),' Oleh: ',bk.vnip_appbd,
					  ' Pada: ',bk.tappbd
				)
		 from plc2.plc2_upb_bahan_kemas bk
			where bk.iupb_id=ID
			order by bk.ibk_id desc limit 1) as bkapp_bd,

		#Pembuatan MBR App PD
		(select
			concat (
					(case
					  when ubm.iapppd_bm=1 then 'Reject'
					  when ubm.iapppd_bm=2 then 'Approve'
					  end),' Oleh: ',ubm.vnip_apppd_bm,
					  ' Pada: ',ubm.tapppd_bm
				)
		 from plc2.plc2_upb_buat_mbr ubm
			where ubm.ifor_id=FOR_ID
			order by ubm.ibuatmbr_id desc limit 1) as buatmbr_pd,

		#Produksi Pilot App PD
		(select
			concat (
					(case
					  when ppd.iapppd_pp=1 then 'Reject'
					  when ppd.iapppd_pp=2 then 'Approve'
					  end),' Oleh: ',ppd.vnip_apppd_pp,
					  ' Pada: ',ppd.tapppd_pp
				)
		 from plc2.plc2_upb_prodpilot ppd
			where ppd.ifor_id=FOR_ID
			order by ppd.iprodpilot_id desc limit 1) as prodpilapp_pd,

		#Cek Dokumen
		(select
			concat (
					(case
					  when ub.iconfirm_dok=0 then 'Waiting for Confirmation'
					  when ub.iconfirm_dok=1 then 'Confirmed'
					  end),' Oleh: ',ub.vnip_confirm_dok,
					  ' Pada: ',ub.tconfirm_dok
				)
		 from plc2.plc2_upb ub
			where ub.iupb_id=ID
			order by ub.iupb_id desc limit 1) as confirmdok_bd,

		#Pra Reg UPB
		concat (
		(case
		  when p.iappbusdev_prareg=1 then 'Reject'
		  when p.iappbusdev_prareg=2 then'Approve'
		  else 'Waiting For Approval'
		  end),' Oleh: ',p.vnip_appbusdev_prareg,
		  ' Pada: ', p.tappbusdev_prareg
		) as 'praregapp',

		 #Setting Prioritas Registrasi
		(select
			concat (
			(case
			  when pr.iappbusdev=1 then 'Reject'
			  when pr.iappbusdev=2 then'Approve'
			  else 'Waiting For Approval'
			  end),' Oleh: ',pr.cnip,
			  ' Pada: ', pr.tappbusdev
		)
		from plc2.plc2_upb_prioritas_reg_detail prd
		inner join plc2.plc2_upb_prioritas_reg pr on pr.iprioritas_id=prd.iprioritas_id and pr.ldeleted=0
		where  prd.iupb_id=ID and prd.ldeleted=0
		order by pr.iyear desc limit 1
		) as setprioreg_app,

		#Registrasi UPB Busdev App
		concat (
		(case
		  when p.iappbusdev_registrasi=1 then 'Reject'
		  when p.iappbusdev_registrasi=2 then'Approve'
		  else 'Waiting For Approval'
		  end),' Oleh: ',p.vnip_appbusdev_registrasi,
		  ' Pada: ', p.tappbusdev_registrasi
		) as regapp_busdev,

			#Launching Produk APP BD
		(select
		concat (
				(case
				  when bk.iappbd_launch=1 then 'Reject'
				  when bk.iappbd_launch=2 then 'Approve'
				  end),' Oleh: ',bk.vnip_appbd_launch,
				  ' Pada: ',bk.tappbd_launch
			)
		 from plc2.plc2_upb_bahan_kemas bk
			where bk.iupb_id=ID
			order by bk.ibk_id desc limit 1) as launchapp_bd,

		#Launching Produk APP DR
		(select
			concat (
					(case
					  when bk.iappdr_launch=1 then 'Reject'
					  when bk.iappdr_launch=2 then 'Approve'
					  end),' Oleh: ',bk.vnip_appdr_launch,
					  ' Pada: ',bk.tappdr_launch
				)
		 from plc2.plc2_upb_bahan_kemas bk
			where bk.iupb_id=ID
			order by bk.ibk_id desc limit 1) as launchapp_dir
from
	plc2.plc2_upb p
WHERE p.ldeleted=0 AND p.iupb_id=$P{iupb_id}
and p.iteambusdev_id LIKE '%$P!{iteambusdev}%' 
and p.iteampd_id LIKE '%$P!{iteampd}%']]>
	</queryString>
